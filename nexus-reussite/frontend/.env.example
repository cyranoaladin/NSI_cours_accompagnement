# Configuration example pour Nexus Réussite - Frontend
# Copy this file to .env.local and fill in your values

VITE_API_BASE_URL=http://localhost:5002
VITE_WS_BASE_URL=ws://localhost:5002
VITE_APP_NAME=Nexus Réussite
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Development settings
VITE_DEBUG_MODE=false
VITE_SHOW_DEVTOOLS=false
VITE_LOG_LEVEL=info

# API endpoints
VITE_AUTH_ENDPOINT=/api/auth
VITE_UPLOAD_ENDPOINT=/api/upload
VITE_DOCUMENTS_ENDPOINT=/api/documents
